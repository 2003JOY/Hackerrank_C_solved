#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
      char ch;
    char s[100];
    char sen[100];
    
    // take character input
    scanf("%c", &ch);
    
    // take string input
    scanf("%s", s);
    
    // consume leftover newline before sentence input
    scanf("\n");
    
    // take sentence input (until newline)
    scanf("%[^\n]%*c", sen);
    
    // output
    printf("%c\n", ch);
    printf("%s\n", s);
    printf("%s\n", sen);
    return 0;
}
