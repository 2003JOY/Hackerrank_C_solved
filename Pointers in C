#include <stdio.h>

void update(int *a,int *b) {
    // Complete this function  
    int sum = *a + *b;              // sum of values
    int diff = *a - *b;             // difference
    if (diff < 0) diff = -diff;     // absolute difference
    
    *a = sum;   // update value of a
    *b = diff;  // update value of b  
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}
